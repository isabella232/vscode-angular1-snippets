{
	"Angular Controller": {
		"prefix": "ng1controller",
		"body": [
			"namespace ${module} {",
			"\t'use strict';",
			"",
			"\texport class ${Controller}Controller {",
			"\t\tstatic $inject: Array<string> = ['${dependency1}'];",
			"\t\tconstructor(private ${dependency1}: ${dependency1Type}) {}",
			"",
			"\t}",
			"",
			"\tangular",
			"\t\t.module('${Module}')",
			"\t\t.controller('${Controller}Controller', ${Controller}Controller);",
			"}"
		]
	},
	"Angular Service": {
		"prefix": "ng1service",
		"body": [
			"namespace ${module} {",
			"\t'use strict';",
			"",
			"\texport interface I${Service} {",
			"\t}",
			"\texport class ${Service} implements I${Service} {",
			"\t\tstatic $inject: Array<string> = ['${dependency1}'];",
			"\t\tconstructor(private ${dependency1}: ${dependency1Type}) {}",
			"",
			"\t}",
			"",
			"\tangular",
			"\t\t.module('${Module}')",
			"\t\t.service('${Service}', ${Service});",
			"}"
		]
	},
    "Angular Component": {
		"prefix": "ng1component",
		"body": [
			"namespace ${module} {",
			"\t'use strict';",
			"",
			"\texport interface I${componentName} {",
			"\t}",
			"\tconst ${componentName}: ng.IComponentOptions = {",
			"\t\tbindings: {},",
			"\t\tcontroller: ${controllerCtrl},",
			"\t\ttemplateUrl: 'path/to/template.html'," ,
			"\t\ttransclude: true",
			"\t};",
            "",
			"\tangular",
			"\t\t.module('${module}')",
			"\t\t.component('${componentName}', ${componentName});",
			"}"
		]
	},
	"Angular Module": {
		"prefix": "ng1module",
		"body": [
			"namespace ${module} {",
			"\t'use strict';",
			"",
			"\tangular.module('${module}', [",
			"\t\t$0",
			"\t]);",
			"}"
		]
	}
}